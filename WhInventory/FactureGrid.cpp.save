#include "FactureGrid.hh"


    FactureGrid::FactureGrid(wxWindows *parent, int id) : wxGrid(parent,id)
    {
           this->CreateGrid(1, 5);

            m_grid->SetColLabelValue(0, wxT("Item"));
            m_grid->SetColLabelValue(1, wxT("Descripcion"));
            m_grid->SetColLabelValue(2, wxT("Cantidad"));
            m_grid->SetColLabelValue(3, wxT("Valor Unitario"));
            m_grid->SetColLabelValue(4, wxT("Subtotal"));
            m_grid->SetReadOnly(0,4,true);
            m_grid->SetGridCursor(0,0);
            m_grid->AutoSize();

    }

    FactureGrid::onEnter(wxKeyEvent &evt)
    {


    }
